<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	
	<servlet>
	<servlet-name>OrganizacionesControllerIndex</servlet-name>
	<servlet-class>organizaciones.OrganizacionesControllerIndex</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>OrganizacionesControllerIndex</servlet-name>
	<url-pattern>/organizaciones</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>OrganizacionesControllerAdd</servlet-name>
	<servlet-class>organizaciones.OrganizacionesControllerAdd</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>OrganizacionesControllerAdd</servlet-name>
	<url-pattern>/organizaciones/add</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>ProductosControllerIndex</servlet-name>
	<servlet-class>productos.ProductosControllerIndex</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>ProductosControllerIndex</servlet-name>
	<url-pattern>/productos</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>ProductosControllerAdd</servlet-name>
	<servlet-class>productos.ProductosControllerAdd</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>ProductosControllerAdd</servlet-name>
	<url-pattern>/productos/add</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>BoletasControllerIndex</servlet-name>
	<servlet-class>boletas.BoletasControllerIndex</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>BoletasControllerIndex</servlet-name>
	<url-pattern>/boletas</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>BoletasControllerAdd</servlet-name>
	<servlet-class>boletas.BoletasControllerAdd</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>BoletasControllerAdd</servlet-name>
	<url-pattern>/boletas/add</url-pattern>
</servlet-mapping>
<servlet>
		<servlet-name>controladorCerrarSession</servlet-name>
		<servlet-class>controller.cerrarSession</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>controladorCerrarSession</servlet-name>
		<url-pattern>/cerrarSession</url-pattern>
	</servlet-mapping>
	
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
